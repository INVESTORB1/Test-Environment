<!doctype html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>Admin - Users</title>
  </head>
  <body>
    <h1>Created Users</h1>
    <p><a href="/admin">Back to Admin</a></p>
    <table>
      <thead>
        <tr><th>Email</th><th>Created At</th></tr>
      </thead>
      <tbody>
        <% users.forEach(u => { %>
          <tr>
            <td><%= u.email %></td>
            <td><%= u.created_at ? (new Date(u.created_at)).toLocaleString() : '-' %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
